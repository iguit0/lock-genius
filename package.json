{
  "name": "lock-genius",
  "version": "0.1.0",
  "private": true,
  "description": "A simple password manager",
  "author": "Igor Alves <igorsk89@gmail.com> (https://github.com/iguit0)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iguit0/lock-genius.git"
  },
  "scripts": {
    "dev": "prisma generate && next dev --turbo",
    "build": "prisma generate && prisma db push --accept-data-loss && next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format:check": "biome format ./src",
    "format:write": "biome format --write ./src",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage --watchAll",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "db:setup": "prisma generate && prisma db push",
    "db:reset": "prisma db push --force-reset",
    "db:studio": "prisma studio"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@t3-oss/env-nextjs": "^0.9.2",
    "axios": "^1.13.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.312.0",
    "motion": "^12.23.26",
    "next": "14.2.35",
    "next-auth": "^4.24.13",
    "next-themes": "^0.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.68.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.25.76"
  },
  "pnpm": {
    "overrides": {
      "@babel/helpers": ">=7.26.10",
      "@babel/runtime": ">=7.26.10",
      "js-yaml": ">=4.1.1",
      "preact": ">=10.27.3"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.27",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^15.5.2",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3"
  }
}
